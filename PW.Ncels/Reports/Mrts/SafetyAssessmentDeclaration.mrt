        <Name>OBK_AssessmentDeclaration</Name>
        <NameInSource>ncelsProd</NameInSource>
        <Parameters isList="true" count="1">
          <value>AssessmentDeclarationId,AssessmentDeclarationId,14,0</value>
        </Parameters>
        <SqlCommand>SELECT ad.*, ad.Id, d.NameRu, dc.AddressLegalRu, dc.AddressFact, dc.Phone,
  dc.BankNameRu, dc.BankIik, dc.BankBik, p.NameRu, p.ProducerNameRu,
  p.CountryNameRu, ps.Series, ps.SeriesEndDate, ps.SeriesParty, p.NameKz
FROM OBK_AssessmentDeclaration ad INNER JOIN
  OBK_Contract c ON ad.Contract_Id = c.Id INNER JOIN
  OBK_Declarant d ON d.Id = c.DeclarantId INNER JOIN
  OBK_DeclarantContact dc ON dc.Id = c.DeclarantContactId LEFT JOIN
  OBK_RS_Products p ON c.Id = p.ContractId LEFT JOIN
  OBK_Procunts_Series ps ON p.Id = ps.OBK_RS_ProductsId
WHERE ad.Id = @AssessmentDeclarationId</SqlCommand>
      </OBK_AssessmentDeclaration>
